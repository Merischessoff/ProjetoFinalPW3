<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/logotipo_fundo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logotipo_fundo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjetoFinalPw3"
        android:usesCleartextTraffic="true"
        >

        <activity android:name=".LoginActivity" android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity >
        <activity android:name=".CadUsuarioActivity"/>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.ProjetoFinalPw3.NoActionBar"
            />
    </application>

</manifest>